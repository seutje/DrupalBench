# Gemini API Key for Task Generation
GEMINI_API_KEY=your_api_key_here

# OpenAI API key
OPENAI_API_KEY=your_api_key_here

# OpenRouter API key
OPENROUTER_API_KEY=your_api_key_here

# Model Provider: gemini, openai, openrouter, or ollama
MODEL_PROVIDER=ollama
# Model name (e.g., gpt-4.1-mini, openai/gpt-4o-mini, llama3, deepseek-coder-v2:16b, gemini-3-flash-preview)
MODEL_NAME=deepseek-coder-v2:16b
# Ollama Host IP/URL
OLLAMA_HOST=http://localhost:11434
# OpenRouter base URL (optional)
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
# Optional OpenRouter attribution headers
# OPENROUTER_HTTP_REFERER=https://your-site.example
# OPENROUTER_X_TITLE=DrupalBench

# Max full prompt size in thousands of tokens (e.g. 32 => 32000 tokens). Default is 128 => 128000.
MAX_TOKENS=128
# Optional hard character cap for assembled code context (defaults to MAX_TOKENS*4000 chars if unset).
# MAX_CONTEXT_CHARS=512000
